{
  "title": "端到端平台验收组-Gitbook",
  "author" : "0027009406 wang.rong",
  "description": "中兴软创-鲲鹏项目-端到端平台验收组-Gitbook",
  "language" : "zh-hans",
  "links": {
	"sidebar":{
		"问题反馈":"https://github.com/wangr0031/mybook/issues"
	}
  },
  "pdf": {
        "fontSize": 12,
        "footerTemplate": null,
        "headerTemplate": null,
        "margin": {
            "bottom": 36,
            "left": 62,
            "right": 62,
            "top": 36
        },
        "pageNumbers": false,
        "paperSize": "a4"
    },
  "plugins": [
	"-search",
	"search-pro",
	"splitter",
	"tbfed-pagefooter",
	"todo",
	"copy-code-button",
	"alerts",
	"simple-page-toc",
	"prism",
	"prism-themes",
	"anchors",
	"donate",
	"local-video",
	"edit-link"
  ],
  "pluginsConfig": {
    "search-pro": {
      "cutWordLib": "nodejieba",
	  "defineWord" : ["Gitbook Use"]
    },
    "tbfed-pagefooter": {
      "copyright":"Copyright &copy 2018 WhaleCloud 鲲鹏项目-端到端平台验收组",
      "modify_label": "最近修订时间: ",
      "modify_format": "YYYY-MM-DD HH:mm:ss"
    },
	"prism": {
      "css": [
         "prism-themes/themes/prism-base16-ateliersulphurpool.light.css"
      ]
    },
	"simple-page-toc": {
      "maxDepth": 3,
      "skipFirstH1": true
    },
	"donate": {
			"wechat": "images/wechatpay.png",
			"alipay": "images/alipay.png",
			"title": "您的支持将鼓励我继续创作！",
			"button": "打赏"
    },
	"edit-link": {
        "base": "https://github.com/wangr0031/mybook/blob/master",
        "label": "Edit This Page"
    }
  }
}
